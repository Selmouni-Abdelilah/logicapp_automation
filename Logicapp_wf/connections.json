        "id": "/subscriptions/@appsetting('subscription')/resourceGroups/@appsetting('resource_group')/providers/Microsoft.Web/connections/slack"
      },
      "authentication": {
        "type": "ManagedServiceIdentity"
      },
      "connectionRuntimeUrl":  "@appsetting('slack_connection_runtime_url')"

    }
  }
